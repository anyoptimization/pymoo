@article{omni_test,
title = "Omni-optimizer: A generic evolutionary algorithm for single and multi-objective optimization",
journal = "European Journal of Operational Research",
volume = "185",
number = "3",
pages = "1062 - 1087",
year = "2008",
issn = "0377-2217",
